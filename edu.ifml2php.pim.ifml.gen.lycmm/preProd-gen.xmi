<?xml version="1.0" encoding="ISO-8859-1"?>
<coreMVC:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:coreMVC="http://www.application.org/coreMVC"
    xmlns:extPHP="http://www.application.org/extPHP"
    name="Movies">
  <aPackageModel
      name="domainModel">
    <models
        name="Movie">
      <attributes
          name="title"/>
      <attributes
          name="year"/>
      <attributes
          name="genre"/>
      <attributes
          name="director"/>
    </models>
  </aPackageModel>
  <aPackageView
      name="InteractionFlowModel">
    <views name="ListMovie">
      <viewComponents
          xsi:type="extPHP:Anchor"
          name="movie"
          tagName="a"
          target="self"
          hypRef="Movie"/>
      <viewComponents
          xsi:type="extPHP:Anchor"
          name="addFormMovie"
          tagName="a"
          target="self"
          hypRef="AddFormMovie"/>
      <viewComponents
          xsi:type="extPHP:Text"
          name="something"
          tagName="p"
          language="es"
          content="Un mensaje"/>
      <innerViews
          name="testWindow">
        <innerViews
            name="anotherTestWindow"/>
      </innerViews>
    </views>
    <views name="Movie">
      <viewComponents
          xsi:type="extPHP:Anchor"
          name="listMovie"
          tagName="a"
          target="self"
          hypRef="ListMovie"/>
      <viewComponents
          xsi:type="extPHP:Anchor"
          name="addFormMovie"
          tagName="a"
          target="self"
          hypRef="AddFormMovie"/>
      <viewComponents
          xsi:type="extPHP:Checkbox"
          name="Vehiculo"
          tagName="input"
          isPairedTag="false"
          type="checkbox"
          value="Trenes"/>
      <viewComponents
          xsi:type="extPHP:Checkbox"
          name="Vehiculo"
          tagName="input"
          isPairedTag="false"
          type="checkbox"
          value="Tractores"/>
      <viewComponents
          xsi:type="extPHP:Checkbox"
          name="Vehiculo"
          tagName="input"
          isPairedTag="false"
          type="checkbox"
          value="Camiones"/>
    </views>
    <views name="AddFormMovie">
      <viewComponents
          xsi:type="extPHP:Anchor"
          name="listMovie"
          tagName="a"
          target="self"
          hypRef="ListMovie"/>
      <viewComponents
          xsi:type="extPHP:Anchor"
          name="movie"
          tagName="a"
          target="self"
          hypRef="Movie"/>
      <viewComponents
          xsi:type="extPHP:Text"
          name="welcome_message"
          tagName="p"
          language="es"
          content="Hola Mundo"/>
      <viewComponents
          xsi:type="extPHP:Text"
          name="title"
          tagName="p"
          language="es"
          content="Supercalifragilisticoespialidoso"/>
      <viewComponents
          xsi:type="extPHP:Image"
          name="img_prueba"
          tagName="img"
          source="/img/smd"/>
      <viewComponents
          xsi:type="extPHP:Form"
          name="AddMovieForm"
          tagName="form"
          method="post"
          target="self">
        <events
            type="onSubmit"
            handler="addMovie"/>
        <htmlElements
            xsi:type="extPHP:TextField"
            name="title"
            tagName="input"
            isPairedTag="false"
            type="text"/>
        <htmlElements
            xsi:type="extPHP:TextField"
            name="year"
            tagName="input"
            isPairedTag="false"
            type="text"/>
        <htmlElements
            xsi:type="extPHP:RadioButton"
            name="Genre"
            tagName="input"
            isPairedTag="false"
            type="radio"
            value="Suspense"/>
        <htmlElements
            xsi:type="extPHP:RadioButton"
            name="Genre"
            tagName="input"
            isPairedTag="false"
            type="radio"
            value="Action"/>
        <htmlElements
            xsi:type="extPHP:Checkbox"
            name="Director"
            tagName="input"
            isPairedTag="false"
            type="checkbox"
            value="Steven Spielberg"/>
        <htmlElements
            xsi:type="extPHP:Checkbox"
            name="Director"
            tagName="input"
            isPairedTag="false"
            type="checkbox"
            value="Gaspar Noé"/>
      </viewComponents>
    </views>
    <views name="UpdateFormMovie">
      <viewComponents
          xsi:type="extPHP:Anchor"
          name="listMovie"
          tagName="a"
          target="self"
          hypRef="ListMovie"/>
      <viewComponents
          xsi:type="extPHP:Anchor"
          name="movie"
          tagName="a"
          target="self"
          hypRef="Movie"/>
      <viewComponents
          xsi:type="extPHP:Text"
          name="welcome_message"
          tagName="p"
          language="es"
          content="Update Movie"/>
      <viewComponents
          xsi:type="extPHP:Text"
          name="title"
          tagName="p"
          language="es"
          content="Supercalifragilisticoespialidoso"/>
      <viewComponents
          xsi:type="extPHP:Image"
          name="img_prueba"
          tagName="img"
          source="/img/smd"/>
      <viewComponents
          xsi:type="extPHP:Form"
          name="UpdateMovieForm"
          tagName="form"
          method="post"
          target="self">
        <events
            type="onSubmit"
            handler="updateMovie"/>
        <htmlElements
            xsi:type="extPHP:TextField"
            name="title"
            tagName="input"
            isPairedTag="false"
            type="text"/>
        <htmlElements
            xsi:type="extPHP:TextField"
            name="year"
            tagName="input"
            isPairedTag="false"
            type="text"/>
        <htmlElements
            xsi:type="extPHP:TextField"
            name="id"
            tagName="input"
            type="text"/>
        <htmlElements
            xsi:type="extPHP:RadioButton"
            name="Genre"
            tagName="input"
            isPairedTag="false"
            type="radio"
            value="Suspense"/>
        <htmlElements
            xsi:type="extPHP:RadioButton"
            name="Genre"
            tagName="input"
            isPairedTag="false"
            type="radio"
            value="Action"/>
        <htmlElements
            xsi:type="extPHP:Checkbox"
            name="Director"
            tagName="input"
            isPairedTag="false"
            type="checkbox"
            value="Steven Spielberg"/>
        <htmlElements
            xsi:type="extPHP:Checkbox"
            name="Director"
            tagName="input"
            isPairedTag="false"
            type="checkbox"
            value="Gaspar Noé"/>
      </viewComponents>
    </views>
    <views name="DeleteFormMovie">
      <viewComponents
          xsi:type="extPHP:Text"
          name="welcome_message"
          tagName="p"
          language="es"
          content="Delete Movie"/>
      <viewComponents
          xsi:type="extPHP:Image"
          name="img_prueba"
          tagName="img"
          source="/img/smd"/>
      <viewComponents
          xsi:type="extPHP:Form"
          name="DeleteMovieForm"
          tagName="form"
          method="post"
          target="self">
        <events
            type="onSubmit"
            handler="deleteMovie"/>
        <htmlElements
            xsi:type="extPHP:TextField"
            name="id"
            tagName="input"
            isPairedTag="false"
            type="text"/>
      </viewComponents>
    </views>
  </aPackageView>
  <aPackageController
      name="domainModel">
    <controllers
        name="Movie">
      <methods
          name="addMovie">
        <inParameters
            name="year"/>
        <inParameters
            name="title"/>
        <inParameters
            name="genre"/>
        <inParameters
            name="director"/>
      </methods>
      <methods
          name="updateMovie"/>
      <methods
          name="deleteMovie"/>
    </controllers>
  </aPackageController>
</coreMVC:Application>
